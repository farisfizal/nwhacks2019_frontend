{
	"name": "NWHacks2019",
	"version": "1.0",
	"description": "Read stuff without actually reading it?",
	"manifest_version": 2,
	"background": {
		"scripts": ["js/jquery-1.10.2.js","js/tippy.js","js/background.js"],
		"persistent": true
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self'",
	"permissions": ["contextMenus"],
	"content_scripts": [{
		"js": ["js/jquery-1.10.2.js",
			"js/tippy.js", "js/inject.js", "js/bootstrap.min.js"],
		"matches": ["<all_urls>"]
	}]
}
