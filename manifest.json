{
	"name": "Insite",
	"version": "1.0",
	"description": "Read stuff without actually reading it?",
	"manifest_version": 2,
	"background": {
		"scripts": ["js/jquery-1.10.2.js","js/tippy.js","js/background.js"],
		"persistent": true
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "icon.png"
	},
	"permissions": ["activeTab"]
}
